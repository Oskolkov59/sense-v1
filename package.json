{
  "name": "@sense-finance/sense-v1",
  "version": "1.2.0",
  "private": true,
  "description": "Sense V1 Contracts",
  "author": "Federico M. Alconada Verzini",
  "homepage": "https://github.com/sense-finance/sense-v1#readme",
  "repository": "git@github.com:sense-finance/sense-v1.git",
  "workspaces": [
    "pkg/*"
  ],
  "scripts": {
    "test": "yarn workspaces foreach --parallel --verbose --interlaced run test | gnomon",
    "test:mainnet": "yarn workspaces foreach --parallel --verbose --interlaced run test:mainnet | gnomon",
    "test:fuzz": "yarn workspace @sense-finance/v1-core run test:fuzz | gnomon",
    "test:4626": "yarn workspace @sense-finance/v1-core run test:4626 | gnomon",
    "lint": "yarn workspaces foreach --parallel --verbose run lint",
    "fix": "yarn workspaces foreach --parallel --verbose run fix",
    "gas-snapshot": "yarn workspaces foreach --parallel --verbose --interlaced run gas-snapshot"
  },
  "packageManager": "yarn@3.1.0",
  "devDependencies": {
    "gnomon": "^1.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5"
  },
  "lint-staged": {
    "**/**/*.{js,sol}": [
      "yarn fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
